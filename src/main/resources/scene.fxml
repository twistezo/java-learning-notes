  <children>
    <AnchorPane fx:id="anchorPane" maxHeight="-1.0" maxWidth="-1.0" prefHeight="318.0" prefWidth="400.0" stylesheets="@style.css" VBox.vgrow="ALWAYS">
      <children>
            <TextArea fx:id="textArea" editable="false" layoutX="28.0" layoutY="295.0" mouseTransparent="true" pickOnBounds="false" stylesheets="@style.css" wrapText="true" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
