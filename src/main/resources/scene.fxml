            <Button fx:id="randButton" layoutX="135.0" layoutY="309.0" mnemonicParsing="false" onAction="#handleRandButtonOnAction" prefWidth="80.0" text="Random" AnchorPane.bottomAnchor="10.0" />
            <Button fx:id="nextButton" layoutX="270.0" layoutY="359.0" mnemonicParsing="false" onAction="#handleNextButtonOnAction" prefWidth="80.0" text="Next" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="30.0" />
